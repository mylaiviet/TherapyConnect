{
  "serviceName": "karematch",
  "containers": {
    "app": {
      "image": ":karematch.latest",
      "ports": {
        "5000": "HTTP"
      },
      "environment": {
        "NODE_ENV": "production",
        "PORT": "5000",
        "AWS_REGION": "us-east-1"
      }
    }
  },
  "publicEndpoint": {
    "containerName": "app",
    "containerPort": 5000,
    "healthCheck": {
      "path": "/health",
      "intervalSeconds": 30,
      "timeoutSeconds": 5,
      "successCodes": "200"
    }
  }
}
